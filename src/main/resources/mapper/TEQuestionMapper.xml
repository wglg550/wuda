<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qmth.wuda.teaching.dao.TEQuestionMapper">

    <delete id="deleteAll">
        delete t from t_e_question t
        <where>
            <if test="paperId != null and paperId != ''">
                and t.paper_id = #{paperId}
            </if>
        </where>
    </delete>

    <select id="findByPaperId" resultType="com.qmth.wuda.teaching.entity.TEQuestion">
        select * from t_e_question teq
        <where>
            <if test="paperId != null and paperId != ''">
                and teq.paper_id = #{paperId}
            </if>
        </where>
    </select>
</mapper>
